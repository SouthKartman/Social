            <Button Content="AIMP" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="292" Click="Button_Click_4"/>
            <Button Content= "Discord" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="292" Click="Button_Click_5"/>
