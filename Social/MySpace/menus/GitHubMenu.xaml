     d:DesignHeight="525" d:DesignWidth="470"
     >

    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button HorizontalAlignment="Center" Height="46" Foreground="#fff" Content="GitHub" FontWeight="Bold" FontSize="16" Width="266" Margin="10" Click="Button_Click"/>
            <Button HorizontalAlignment="Center" Height="46" Foreground="#fff" Content="GitHub Desktop" FontWeight="Bold" FontSize="16" Width="266" Margin="10" Click="Button_Click_1"/>
            <Button HorizontalAlignment="Center" Height="46" Foreground="#fff" Content="GitBush" FontWeight="Bold" FontSize="16" Width="266" Margin="10" Click="Button_Click_2"/>
        </StackPanel>
